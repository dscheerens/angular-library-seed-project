{
  "name": "example-module",
  "version": "5.0.0",
  "description": "A seed project Angular 5+ library projects",
  "homepage": "https://github.com/dscheerens/angular-library-seed-project",
  "author": "Daan Scheerens",
  "license": "MIT",
  "keywords": [
    "angular",
    "angular5",
    "ngx",
    "library",
    "seed",
    "starter",
    "skeleton",
    "project",
    "module"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dscheerens/angular-library-seed-project.git"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --app=demo",
    "lint": "tslint -p lib/tsconfig.lib.json -t stylish -c tslint.json",
    "test": "ng test --app=lib --single-run --code-coverage",
    "test.watch": "ng test --app=lib --code-coverage",
    "test.aot": "ng build --prod --app=demo",
    "build": "ts-node ng-packagr.ts",
    "publish.dist": "npm publish build/dist"
  },
  "ngPackage": {
    "dest": "build/dist",
    "lib": {
      "entryFile": "lib/public_api.ts",
      "umdId": "exampleModule",
      "umdModuleIds": {}
    }
  },
  "peerDependencies": {
    "@angular/common": ">=5.0.0",
    "@angular/core": ">=5.0.0",
    "rxjs": ">=5.5.0",
    "tslib": ">=1.8.0"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/common": "^5.2.10",
    "@angular/compiler": "^5.2.10",
    "@angular/compiler-cli": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@dscheerens/tslint-presets": "^1.0.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/lodash": "^4.14.108",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.1.0",
    "core-js": "^2.4.1",
    "ci-info": "^1.1.3",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-mocha-reporter": "^2.2.5",
    "lodash": "^4.17.10",
    "ng-packagr": "^2.4.2",
    "rxjs": "^5.5.10",
    "ts-node": "^6.0.1",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "tslint-language-service": "^0.9.9",
    "typescript": "~2.7.2",
    "zone.js": "^0.8.26"
  }
}
